 */
function displayWords(){
	var wordList=["a","b","c"];
	for (i=0;i<wordList.length;setTimeout(i++,30000)){
		window.alert(wordList[i]);
		//document.write(wordList[i]);
		
	}
	
}
function writeWords(j){
	(function(j) {
        setTimeout(function() { document.write(wordList[j]); }, j * 3000);
    })(j);
}

function yo(){
	var wordList=["a","b","c"];
	for (var j = 0; j < wordList.length; j++) {
	     writeWords(j);
	}
}
function x(){
	var wordList=["a","b","c"];
	for (var i = 0; i < wordList.length; i++) {
	    (function(index) {
	        setTimeout(function() { document.write(wordList[index]); }, i * 3000);
	    })(i);
	}
}

